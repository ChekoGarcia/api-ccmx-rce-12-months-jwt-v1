<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="Upsert">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>AM-DELETE-PATHSUFGFIX</Name>
            </Step>
        </Request>
        <Response>
            <Step>
                <Name>AM-AddCORS</Name>
            </Step>
        </Response>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties>
            <Property name="success.codes">1xx,2xx,3xx,4xx,5xx</Property>
        </Properties>
        <LoadBalancer>
            <Server name="target_pdmx"/>
        </LoadBalancer>
        <Path>{private.context.sf}/settings-cf/upsert</Path>
    </HTTPTargetConnection>
</TargetEndpoint>